class Solution {
    public void reverseString(char[] s) {

        char[] st = new char[s.length];
        int k = 0;

        for (int i = s.length - 1; i >= 0; i--) {
            st[i] = s[k];
            k++;
        }

        for (int i = 0; i < s.length; i++) {
            s[i] = st[i];
        }
    }
}
